SUBDIRS='error lwunistr memory file dscache time logging'

make()
{
	mk_multiarch_do
            mk_library \
                LIB="lwadvapi_nothr" \
                GROUPS="\
                    error/error \
                    file/file \
                    dscache/dscache \
                    memory/memory \
                    lwunistr/lwunistr \
                    time/time \
                    logging/logging"
	mk_multiarch_done
}